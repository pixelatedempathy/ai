# Journal Dataset Research System - Environment Variables
# Copy this file to .env and fill in your API credentials

# NCBI E-utilities (PubMed Central)
# Register for API key at: https://www.ncbi.nlm.nih.gov/account/
NCBI_API_KEY=your_ncbi_api_key_here
NCBI_EMAIL=your_email@example.com

# DOAJ (Directory of Open Access Journals)
# API key is optional, register at: https://doaj.org/account/login
DOAJ_API_KEY=

# Dryad Data Repository
# API key is optional, register at: https://datadryad.org/
DRYAD_API_KEY=

# Zenodo Research Repository
# Access token is optional, create at: https://zenodo.org/account/settings/applications/
ZENODO_ACCESS_TOKEN=

# ClinicalTrials.gov
# No API key required

# Encryption Key for Acquired Datasets
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
DATASET_ENCRYPTION_KEY=
