# Example environment variables for AI components
# Copy this file to .env and fill in secrets before running local scripts

# Hugging Face Hub token with 'read' access for private model downloads
HUGGINGFACE_HUB_TOKEN=your_huggingface_hub_token_here

# OVH Cloud object storage (S3-compatible) for training datasets
# These are the canonical vars used by training/VPS scripts.
OVH_S3_BUCKET=pixel-data
OVH_S3_ENDPOINT=https://s3.us-east-va.io.cloud.ovh.us
OVH_S3_REGION=us-east-va
OVH_S3_ACCESS_KEY=your_ovh_access_key_here
OVH_S3_SECRET_KEY=your_ovh_secret_key_here

# Convenience alias so older scripts that expect S3_BUCKET keep working
S3_BUCKET=${OVH_S3_BUCKET}
